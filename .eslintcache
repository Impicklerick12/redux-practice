\n// ALL REDUCERS IN ONE OBJECT, IMPORTED FROM DUCKS\nconst reducer = combineReducers({\n    counter: counterReducer,\n    user: userReducer,\n    activities: activitiesReducer,\n    asyncActions: asyncActionsReducer,\n    posts: postsReducer,\n    bitcoin: bitcoinReducer\n});\n\n// STORE - CREATE STORE THROUGH REDUX, USING ALL REDUCERS, APPLYING MIDDLEWARE\nconst store = createStore(reducer, applyMiddleware(thunk));\n\nexport default store","/Users/tylerhall/redux-practice/src/redux/sagas/handlers/user.js",[],["123","124"],"/Users/tylerhall/redux-practice/src/redux/ducks/activities.js",["125"],"/Users/tylerhall/redux-practice/src/redux/sagas/requests/user.js",[],"/Users/tylerhall/redux-practice/src/Exports.js",[],"/Users/tylerhall/redux-practice/src/components/WorkoutTracker.js",[],"/Users/tylerhall/redux-practice/src/components/Activities/Workouts.js",[],"/Users/tylerhall/redux-practice/src/components/Activities/AddActivity.js",[],"/Users/tylerhall/redux-practice/src/index.js",[],["126","127"],"/Users/tylerhall/redux-practice/src/redux/ducks/counter.js",["128"],"const INCREMENT = 'increment'\nconst DECREMENT = 'decrement'\nconst RESET = 'reset'\n\nexport const increment = () => ({\n    type: INCREMENT\n})\n\nexport const decrement = () => ({\n    type: DECREMENT\n})\n\nexport const reset = () => ({\n    type: RESET\n})\n\nconst initialState = {\n    count: 0\n}\n\nexport default (state = initialState, action) => {\n    switch (action.type) {\n        case INCREMENT:\n            return {\n                ...state,\n                count: state.count + 1\n            }\n        case DECREMENT:\n            return {\n                ...state,\n                count: state.count - 1\n            }\n        case RESET:\n            return {\n                ...state,\n                count: 0\n            }\n        default: {\n            return state\n        }\n    }\n}",["129","130"],"/Users/tylerhall/redux-practice/src/Counter.js",[],"/Users/tylerhall/redux-pract