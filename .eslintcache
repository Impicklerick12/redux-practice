/src/App.js",["66"],"/Users/tylerhall/redux-practice/src/components/CounterComponent.js",["67"],"import React, { useEffect } from 'react'\nimport '../styles/app.css'\nimport { useSelector } from 'react-redux'\nimport Counter from '../Counter'\n\nimport { useDispatch } from 'react-redux'\nimport { reset } from '../redux/ducks/counter' \n\nconst CounterComponent = () => {\n\n    const dispatch = useDispatch();\n\n    const count = useSelector(state => state.counter.count);\n    const user = useSelector(state => state.user.user);\n\n    const names = [\"Tyler\", \"Bob\", \"Foo\"]\n\n    // reset component only resets the total count, not individual votes\n    const handleReset = () => {\n        dispatch(reset())\n      }\n\n    return (\n        <div className=\"counter\">\n            <div className=\"heading\">\n                <h1>REDUX PRACTICE</h1>\n            </div>\n            { user && (\n                <div>\n                <h1>Hello, {user.firstName}</h1>\n                </div>\n            )}\n            <div className=\"count\">\n                <h2>The total count is: <strong>{count}</strong></h2>\n            </div>\n            <div className=\"counters\">\n                { names.map((name, i) => \n                <Counter key={i} name={name} />\n                )}\n            </div>\n            <button onClick={handleReset} style={{textAlign: \"center\"}}>Reset</button>\n        </div>\n    )\n}\n\nexport default CounterComponent\n",["68","69"],"/Users/tylerhall/redux-practice/src/redux/configureStore.js",["70"],"import { combineReducers, createStore, applyMiddleware } from 'redux';\nimport { composeWithDevTools } from 'redux-devtools-extension'\n\n// DUCKS REDUCERS\nimport counterReducer from './ducks/counter'\nimport userReducer from './ducks/user'\nimport activitiesReducer from './ducks/activities'\n\n// REDUX SAGA\nimport createSagaMiddleware from 'redux-saga'\nimport { watcherSaga } from './sagas/rootSaga'\n\nconst reducer = combineReduce