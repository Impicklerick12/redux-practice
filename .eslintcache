// REDUX SAGA MIDDLEWARE\nimport createSagaMiddleware from 'redux-saga'\nimport { watcherSaga } from './sagas/rootSaga'\n\n// REDUX THUNK MIDDLEWARE\nimport thunk from 'redux-thunk'\n\n// ALL REDUCERS IN ONE OBJECT, IMPORTED FROM DUCKS\nconst reducer = combineReducers({\n    counter: counterReducer,\n    user: userReducer,\n    activities: activitiesReducer,\n    asyncActions: asyncActionsReducer,\n    posts: postsReducer,\n    bitcoin: bitcoinReducer\n});\n\n// STORE - CREATE STORE THROUGH REDUX, USING ALL REDUCERS, APPLYING MIDDLEWARE\nconst store = createStore(reducer, applyMiddleware(thunk));\n\nexport default store","/Users/tylerhall/redux-practice/src/redux/sagas/handlers/user.js",[],"/Users/tylerhall/redux-practice/src/redux/ducks/activities.js",["130"],"import { v4 as uuidv4 } from 'uuid'\n\nconst initialState = [{\n    id: 1,\n    name: \"Workout\",\n    duration: \"2 hours\"\n}];\n\nexport default (state = initialState, action) => {\n    const { type, payload } = action\n\n    switch (type) {\n        case \"CREATE_ACTIVITY\":\n            return [\n                ...state, {\n                    // assign uniwque id using uuid extension\n                    id: uuidv4(),\n                    name: payload.name,\n                    duration: payload.duration\n                }\n            ]\n        case \"DELETE_ACTIVITY\":\n            // create a copy state, to alter immutably\n            let copyState = [...state]\n            // filter all activities to a new variable, removing the activity id coming from payload\n            let updatedActivities = copyState.filter((a) => a.id !== payload.id)\n            return [\n                ...updatedActivities\n            ]\n        default: {\n            return state\n        }\n    }\n}",["131","132"],"/Users/tylerhall/redux-practice/src/redux/sagas/requests/user.js",[],"/Users/tylerhall/redux-practice/src/Exports.js",[],"/Users/tylerhall/redux-practice/src/components/WorkoutTracker.js",[],"/Users/tylerhall/redux-practice/src/components/Activities/Workouts.js",[],"/Users/tylerhall/redux-practice/src/components/Activities/AddActivity.js",[],"/Users/tylerhall/redux-practice/src/index.js",[],["133","134"],"/Users/tylerhall/redux-practice/src/redux/ducks/counter.js",["135"],"const INCREMENT = 'increment'\nconst DECREMENT = 'decrement'\nconst RESET = 'reset'\n\nexport const increment = () => ({\n    type: INCREMENT\n})\n\nexport const decrement = () => ({\n    type: DECREMENT\n})\n\nexport const reset = () => ({\n    type: RESET\n})\n\nconst initialState = {\n    count: 0\n}\n\nexport default (state = initialState, action) => {\n    switch (action.type) {\n        case INCREMENT:\n            return {\n                ...state,\n                count: state.count + 1\n            }\n        case DECREMENT:\n            return {\n                ...state,\n                count: state.count - 1\n            }\n        case RESET:\n            return {\n                ...state,\n                count: 0\n            }\n        default: {\n            return state\n        }\n    }\n}",["136","137"],"/Users/tylerhall/redux-practice/src/Counter.js",[],"/Users/tylerhall/redux-practice/src/redux/ducks/user.js",["138"],"/Users/tylerhall/redux-practice/src/components/Activities/Activity.js",["139"],"import React from 'react'\nimport { useDispatch, useSelector } from 'react-redux'\n\nconst Activity = (props) => {\n\n    const dispatch = useDispatch();\n    const activities = useSelector(state => state.activities)\n\n    const deleteActivity = (event) => {\n        event.preventDefault()\n\n        dispatch({\n            type: \"DELETE_ACTIVITY\",\n            payload: {\n                id: props.id\n            }\n        })\n    }\n\n    return (\n        <div className=\"activity-wrapper\">\n            <p>Activity: {props.name}</p>\n            <p>Duration: {props.duration}</p>\n            <button onClick={deleteActivity}>Delete</button>\n        </div>\n    )\n}\n\nexport default Activity","/Users/tylerhall/redux-practice/src/redux/sagas/rootSaga.js",[],"/Users/tylerhall/redux-practice/src/redux/ducks/asyncActions.js",[],"/Users/tylerhall/redux-practice/src/components/ThunkUsers.js",[],"/Users/tylerhall/redux-practice/src/redux/thunk/requests/users.js",[],"/Users/tylerhall/redux-practice/src/components/Posts.js",["140"],"import React, { useEffect } from 'react'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { fetchPosts } from '../redux/thunk/requests/posts'\nimport Post from './Post'\n\nconst Posts = () => {\n\n    const dispatch = useDispatch();\n    const posts = useSelector(state => state.posts)\n\n    useEffect(() => {\n        dispatch(fetchPosts());\n    }, [])\n\n    return (\n        <div>\n            <h1>Posts</h1>\n            <div class=\"posts\">\n                { posts.map((post) => {\n                    return <Post key={post.id} post={post} />\n                })}\n            </div>\n        </div>\n    )\n}\n\nexport default Posts\n","/Users/tylerhall/redux-practice/src/redux/ducks/posts.js",["141"],"const initialState = []\n\nexport default (state = initialState, action) => {\n    switch (action.type) {\n        case \"FETCH_POSTS\":\n            return action.payload\n        default:\n            return state\n    }\n}","/Users/tylerhall/redux-practice/src/redux/thunk/requests/posts.js",[],["142","143"],"/Users/tylerhall/redux-practice/src/components/Post.js",[],"/Users/tylerhall/redux-practice/src/components/btcTracker.js",["144"],"/Users/tylerhall/redux-practice/src/components/BTCTracker.js",["145"],"import React, { useState } from 'react'\nimport { Line } from 'react-chartjs-2'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { fetchData } from '../redux/thunk/requests/bitcoin'\n\nconst BTCTracker = () => {\n\n    const dispatch = useDispatch();\n    const state = useSelector(state => state.bitcoin)\n\n    const [number, setNumber] = useState(100)\n\n    const data = {\n